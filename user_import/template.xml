            <per:FirstName><![CDATA[__FIRSTNAME__]]></per:FirstName>
            <per:Salutation><![CDATA[Mr]]></per:Salutation>
            <per:Accesses>
                <per:Brand>
                    <per:BrandCode>__BRAND__</per:BrandCode>
                    <per:Stores>
                        <per:Store>
                            <per:SAPID>__B2BUNIT__</per:SAPID>
                            <per:ShopCode>__STORE__</per:ShopCode>
                            <per:Roles>
                                <per:Role>__ROLE__</per:Role>
                            </per:Roles>
                        </per:Store>
                    </per:Stores>
                </per:Brand>
            </per:Accesses>
            <per:Telephone><![CDATA[513-860-3218 . 5148]]></per:Telephone>
            <per:Mobile/>
            <per:SiebelID/>
            <per:AccountStatus><![CDATA[ACTIVE]]></per:AccountStatus>
        </per:PersonRequest>
    </S:Body>
</S:Envelope>