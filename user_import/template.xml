            <wsse:UsernameToken>
                <wsse:Username>__SECUSERNAME__</wsse:Username>
                <wsse:Password
                        Type="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-username-token-profile-1.0#PasswordText">__SECPASSWORD__</wsse:Password>
            </wsse:UsernameToken>
        </wsse:Security>
    </S:Header>
    <S:Body>
        <per:PersonRequest>
            <per:HybrisID/>
            <per:EmailAddress><![CDATA[__EMAIL__]]></per:EmailAddress>
            <per:Password><![CDATA[__ENCRYPTEDPASSWORD__]]></per:Password>
